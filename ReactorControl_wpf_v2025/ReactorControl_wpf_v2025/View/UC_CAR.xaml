<UserControl x:Class="ReactorControl.View.UC_CAR"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ReactorControl.View"
             mc:Ignorable="d" 
             d:DesignHeight="582" d:DesignWidth="110">
    <Grid>
        <Label x:Name="CAR_one_BG" Content="Label" HorizontalAlignment="Center" VerticalAlignment="Center" Background="Red" Height="582" Width="110"/>
        <Button x:Name="CAR_one_value_label_1" HorizontalAlignment="Center" VerticalAlignment="Top" HorizontalContentAlignment="Center" Height="20" Width="110" Background="White" BorderBrush="White">
            <Label x:Name="CAR_one_value_value_1" Content="0.00" HorizontalAlignment="Left" HorizontalContentAlignment="Right" Background="#FFE0E0E0" Width="100" Height="20" VerticalAlignment="Top" Margin="0,-2,0,0" FontSize="8"/>
        </Button>
        <Button x:Name="CAR_one_value_label_2" HorizontalAlignment="Center" VerticalAlignment="Top" HorizontalContentAlignment="Center" Height="20" Width="110" Background="White" BorderBrush="White" Margin="0,22,0,0">
            <Label x:Name="CAR_one_value_value_2" Content="0.00" HorizontalAlignment="Left" HorizontalContentAlignment="Right" Background="#FFE0E0E0" Width="100" Height="20" VerticalAlignment="Top" Margin="0,-2,0,0" FontSize="8"/>
        </Button>
        <Button x:Name="CAR_one_value_label_3" HorizontalAlignment="Center" VerticalAlignment="Top" HorizontalContentAlignment="Center" Height="20" Width="110" Background="White" BorderBrush="White" Margin="0,44,0,0">
            <Label x:Name="CAR_one_value_value_3" Content="0.00" HorizontalAlignment="Left" HorizontalContentAlignment="Right" Background="#FFE0E0E0" Width="100" Height="20" VerticalAlignment="Top" Margin="0,-2,0,0" FontSize="8"/>
        </Button>
        <Button x:Name="CAR_one_btn_up" Content="UP" HorizontalAlignment="Center" VerticalAlignment="Top" Height="20" Width="110" Background="#FF9D9D9D" BorderBrush="#FF9D9D9D" Margin="0,66,0,0"/>
        <Button x:Name="CAR_one_up_btn_one" Content="1" HorizontalAlignment="Left" Margin="0,88,0,0" VerticalAlignment="Top" Width="54" Height="20" Background="#FF9D9D9D" BorderBrush="#FF9D9D9D"/>
        <Button x:Name="CAR_one_up_btn_two" Content="2" HorizontalAlignment="Left" Margin="56,88,0,0" VerticalAlignment="Top" Width="54" Height="20" Background="#FF9D9D9D" BorderBrush="#FF9D9D9D"/>
        <Label x:Name="CAR_one_cylinder_up" Content="" HorizontalAlignment="Center" Margin="0,275,0,0" VerticalAlignment="Top" Background="#FF4D4D4D" Width="56" Height="5"/>
        <Label x:Name="CAR_one_cylinder_middle_small" Content="" HorizontalAlignment="Center" Margin="0,110,0,0" VerticalAlignment="Top" Background="#FFAEAEAE" Width="22" Height="165"/>
        <Label x:Name="CAR_one_cylinder_middle_large" HorizontalAlignment="Center" Margin="0,280,0,0" VerticalAlignment="Top" Background="#FFE0E0E0" Width="56" Height="187">
            <Label x:Name="CAR_one_name" Content="CAR1" HorizontalAlignment="Center" HorizontalContentAlignment="Center" Background="#FFE0E0E0" Width="50" Height="28" VerticalAlignment="Center" Margin="0,73,0,0"/>
        </Label>
        <Label x:Name="CAR_one_cylinder_bottom" Content="" HorizontalAlignment="Center" Margin="0,467,0,0" VerticalAlignment="Top" Background="#FF4D4D4D" Width="56" Height="5"/>
        <Button x:Name="CAR_one_down_btn_one" Content="1" HorizontalAlignment="Left" Margin="0,474,0,0" VerticalAlignment="Top" Width="54" Height="20" Background="#FF00A01E" BorderBrush="#FF00A01E"/>
        <Button x:Name="CAR_one_down_btn_two" Content="2" HorizontalAlignment="Right" Margin="0,474,0,0" VerticalAlignment="Top" Width="54" Height="20" Background="#FF00A01E" BorderBrush="#FF00A01E"/>
        <Button x:Name="CAR_one_down_btn_down" Content="DOWN" HorizontalAlignment="Center" Margin="0,496,0,0" VerticalAlignment="Top" Width="110" Height="20" Background="#FF00A01E" BorderBrush="#FF00A01E"/>
        <Button x:Name="CAR_one_down_btn_resest_" Content="RESET" HorizontalAlignment="Center" Margin="0,496,0,44" VerticalAlignment="Bottom" Width="110" Height="20" Background="#FF00A01E" BorderBrush="#FF00A01E"/>
        <Button x:Name="CAR_one_btn_freeze" Content="FREEZE" HorizontalAlignment="Center" VerticalAlignment="Bottom" Height="20" Width="110" Background="#FF9D9D9D" BorderBrush="#FF9D9D9D" Margin="0,61,0,22"/>
        <Button x:Name="CAR_one_btn_uunfreeze" Content="UNFREEZE" HorizontalAlignment="Center" VerticalAlignment="Bottom" Height="20" Width="110" Background="#FF9D9D9D" BorderBrush="#FF9D9D9D" Margin="0,518,0,0"/>

    </Grid>
</UserControl>
